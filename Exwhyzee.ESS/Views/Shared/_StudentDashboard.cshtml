    <script type="text/jscript">
        $(function () {
            $('#originalstate').change(function () {
                $.getJSON('/Account/LgaList/' + $('#originalstate').val(), function (data) {
                    var items = '<option>Update Local Government</option>';
                    $.each(data, function (i, local) {
                        items += "<option value='" + local.Value + "'>" + local.Text + "</option>";
                    });
                    $('#LGA').html(items);
                });
            });
        });
    </script>
