    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="" class="logo">
                <span class="logo-mini"><b>ISkools</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>ISkools</b></span>

            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->


                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                @*<a>
                        @Html.Action("LayoutSchoolName", "PartialView", new { Area = "Admin" })
                    </a>*@

                <div class="navbar-custom-menu" style="float:none !important;margin-left:30px !important;">

                </div>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @using (Html.BeginForm("LogOff", "Auth", new { area = "" }, FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @Html.AntiForgeryToken()
                            <li class="dropdown user user-menu">
                                <a href="javascript:document.getElementById('logoutForm').submit()" style="position: relative;color:#fff; text-decoration:none; display: block;padding: 10px 15px;">

                                    Sign Out
                                </a>

                            </li>
                        }

                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                @Html.Action("_DashImage", "Account", new { area = "Student" })
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Advance Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->

                <ul class="sidebar-menu" data-widget="tree">


                    <li class="header">MENU NAVIGATION</li>
                    <li><a href="@Url.Action("Index", "Account", new { area="Dashboard"})"><i class="fa fa-dashboard"></i> <span>Dashboard</span></a></li>
                    <li><a href="/"><i class="fa fa-home"></i> <span>Home Page</span></a></li>


                    <li style="background-color:#dd4b39;color:#000000;" class="treeview">
                        <a href="#">
                            <i class="fa fa-book"></i> <span> Online Class <span class="label label-danger">New</span></span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li style="background-color:#dd4b39;color:#000000;"><a href="@Url.Action("LiveList", "OnlineClass", new { area="Student"})"><i class="fa fa-book"></i> <span> Live Class <span class="label label-danger">New</span></span></a></li>
                            <li><a href="@Url.Action("Index", "Subjects", new { area="Student"})"><i class="fa fa-circle-o"></i> <span>My Subjects</span></a></li>
                            <li><a href="@Url.Action("MySubscription", "Payment", new { area="Finances"})"><i class="fa fa-circle-o"></i> <span>My Subscriptions</span></a></li>

                        </ul>
                    </li>
                  
                    <li><a href="@Url.Action("Profile", "Account", new { area="Student"})"><i class="fa fa-user"></i> <span>Profile</span></a></li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-gamepad"></i> <span> Games</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            @Html.Action("_CategoryMenu", "Initializer", new { area = "Games" })

                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-fighter-jet"></i> <span> Simulations</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            @Html.Action("_SubjectMenu", "Initializer", new { area = "Simulations" })
                        </ul>
                    </li>

                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-book"></i> <span> E-Library</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            @Html.Action("_CategoryMenu", "Ebooks", new { area = "ELibrary" })
                        </ul>
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
