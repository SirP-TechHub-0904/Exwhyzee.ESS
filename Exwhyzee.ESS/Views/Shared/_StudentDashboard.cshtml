    <style>
        .example-modal .modal {
            position: relative;
            top: auto;
            bottom: auto;
            right: auto;
            left: auto;
            display: block;
            z-index: 1;
        }

        .example-modal .modal {
            background: transparent !important;
        }

        .direct-chat-messages {
            height: auto !important;
        }

        .row {
            margin-right: 0px !important;
            margin-left: 0px !important;
        }

