@model IEnumerable<Exwhyzee.ESS.Models.Entities.SimulationSubject>

@foreach (var m in Model)
{
    <li class="treeview">
        <a href="#">
            <i class="fa fa-building-o"></i> <span> @m.Name</span>
            <span class="pull-right-container">
                <i class="fa fa-angle-left pull-right"></i>
            </span>
        </a>
        <ul class="treeview-menu">
            @Html.Action("_CategoryMenu", "Initializer", new { area = "Simulations", id = m.Id })

        </ul>
    </li>

}